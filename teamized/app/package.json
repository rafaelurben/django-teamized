  "funding": "https://github.com/sponsors/rafaelurben/",
  "devDependencies": {
    "@eslint/js": "~9.26.0",
    "@types/bootstrap": "~5.2.10",
    "@types/jquery": "~3.5.32",
    "@types/node": "^22.9.0",
    "eslint": "~9.26.0",
    "eslint-config-prettier": "~10.1.0",
    "eslint-plugin-prettier": "~5.3.0",
    "eslint-plugin-react": "~7.37.2",
    "eslint-plugin-react-hooks": "~5.2.0",
    "eslint-plugin-simple-import-sort": "~12.1.1",
    "globals": "~16.0.0",
    "prettier": "~3.5.0",
    "sweetalert2": "11.21.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.0",
    "typescript-eslint": "~8.31.0",
    "webpack": "~5.99.0",
    "webpack-cli": "~6.0.0"
  },
  "dependencies": {
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "bootstrap": "5.3.6",
    "jquery": "3.7.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "2.15.3"
  }
