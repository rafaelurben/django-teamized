{
  "name": "teamized-reactapp",
  "version": "1.0.0",
  "description": "The build commands can also be run via 'python manage.py teamized_build [--live]'",
  "main": "src/app.tsx",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx --no -- webpack --mode production",
    "build-live": "npx --no -- webpack --mode development --watch --devtool eval-source-map",
    "prettier-check": "npx --no -- prettier src --check",
    "prettier-fix": "npx --no -- prettier src --write",
    "eslint-check": "npx --no -- eslint src",
    "eslint-fix": "npx --no -- eslint --fix src",
    "typescript-check": "npx --no -- tsc --noEmit"
  },
